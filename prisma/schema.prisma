generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cadastro_funcionario{
  id Int @id @default(autoincrement())
  nome String
  cpf String
  turno String
  alojamento String
}

model cadastro_alojamento{

  id Int @id @default(autoincrement())
  alojamento String
  cep Int
  rua String
  bairro String
  numero Int
}

model Usuario {
  id    Int    @id @default(autoincrement())
  nome  String
  email String @unique
  password String
  sexo String
  dataCriada DateTime
}

model cadastro_veiculo{

  id Int @id @default(autoincrement())
  placa String
  garagem String
  marca String
  ano Int
  tipo String

}